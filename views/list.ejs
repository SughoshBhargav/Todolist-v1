<%- include("header") -%>
    <div class="box" id = "heading">
        <h1><%=listTitle%></h1>
    </div>
    
    <div class="box">
        <% for(var i=0; i<new_item.length; i++) { %>
        <div class="item">
            <input type="checkbox">
            <p><%=new_item[i]%></p>
        </div>
        <% } %>
    
        <form action="/" method="post" class="item">
            <input type="text" name="new_item" placeholder="New Item"  autocomplete="off">
            <button type="submit" name  ="list" value="<%=listTitle%>">+</button>
        </form>
    </div>
<%- include("footer") -%>